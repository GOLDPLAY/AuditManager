<?xml version="1.0" encoding="utf-8"?>
<ServiceApplication
  Name="LightSwitchServiceApplication"
  Version="1.0.0.0" xmlns="http://schemas.microsoft.com/LightSwitch/2010/xaml/model">
  <ServiceApplication.Attributes>
    <ModuleNamespace
      Namespace="LightSwitchApplication" />
    <Partial
      IsPrimary="True" />
  </ServiceApplication.Attributes>
  <EntityContainerGroup
    Name="DataWorkspace">
    <EntityContainerGroupProperty
      Name="SecurityData"
      EntityContainer="Microsoft.LightSwitch.Security:SecurityData">
      <EntityContainerGroupProperty.Attributes>
        <Generated />
        <DisplayName
          Value="Security Data" />
        <Description
          Value="Service that provides access to security-related data." />
      </EntityContainerGroupProperty.Attributes>
    </EntityContainerGroupProperty>
    <EntityContainerGroupProperty
      Name="ApplicationData"
      EntityContainer="ApplicationData" />
  </EntityContainerGroup>
  <GlobalAttributeGroup
    Name="LightSwitchServiceApplicationGroup">
    <GlobalAttributeGroup.Attributes>
      <AutomaticDisplayName
        Expression="(\p{Ll})(\p{Lu})|_+"
        Replacement="$1 $2" />
    </GlobalAttributeGroup.Attributes>
  </GlobalAttributeGroup>
  <Permission
    Name="CanEditAuditAuditors">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Audit Auditors" />
      <Description
        Value="Can Edit Audit Auditors" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditAuditFindings">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Audit Findings" />
      <Description
        Value="Can Edit Audit Findings" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditAuditors">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Auditors" />
      <Description
        Value="Can Edit Auditors" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditAudits">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Audits" />
      <Description
        Value="Can Edit Audits" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditAuditStatuses">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Audit Statuses" />
      <Description
        Value="Can Edit Audit Statuses" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditAuditTypes">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Audit Types" />
      <Description
        Value="Can Edit Audit Types" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditEmployees">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Employees" />
      <Description
        Value="Can Edit Employees" />
    </Permission.Attributes>
  </Permission>
  <Permission
    Name="CanEditFindingActions">
    <Permission.Attributes>
      <DisplayName
        Value="Can Edit Finding Actions" />
      <Description
        Value="Can Edit Finding Actions" />
    </Permission.Attributes>
  </Permission>
</ServiceApplication>